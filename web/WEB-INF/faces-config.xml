<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <managed-bean>
        <managed-bean-name>nerkhBean</managed-bean-name>
        <managed-bean-class>mnm.data.nerkh</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>computeBean</managed-bean-name>
        <managed-bean-class>mnm.computation.compute</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>convertorBean</managed-bean-name>
        <managed-bean-class>mnm.computation.convertor</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ChartBean</managed-bean-name>
        <managed-bean-class>mnm.computation.chart</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


</faces-config>
